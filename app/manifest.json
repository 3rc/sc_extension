{
  "name": "SoundCloud ++",
  "permissions": [ "tabs", "\u003Call_urls>" ],
  "version": "0.1",
  "manifest_version": 2,
  "description": "For a better SoundCloud Experience",
  "web_accessible_resources": ["images/scpluspluswidesmall.png"],
  "icons": {
    "128": "images/scplusplus.png"
  },
  "content_scripts": [ {
    "js": [
      "scripts/popup.js",
      "scripts/jquery.2.1.3-min.js",
      "scripts/underscore-min.js"],
    "css": ["styles/main.css"],
    "matches": ["https://soundcloud.com/*"],
    "run_at": "document_end"
    }],
  "default_locale": "en",
  "background": {
    "scripts": [
      "scripts/chromereload.js",
      "scripts/background.js"
    ]
  },
  "browser_action": {
    "default_icon": {
      "19": "images/scplusplus.png",
      "38": "images/scplusplus.png"
    },
    "default_title": "SoundCloud Enhancer",
    "default_popup": "popup.html"
  }
}
